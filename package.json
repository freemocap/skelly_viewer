{
  "name": "skellycam-tauri-nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "tauri": "tauri",
    "dev": "cd src-nuxt && npm run dev",
    "run-python": "python src-python/main.py",
    "tauri-dev": "npm run tauri dev",
    "build": "tauri build",
    "nuke-install": "git clean -dfx && npm install",
    "nuke-n-go": "git clean -dfx && npm install && npm run dev",
    "install-nuxt": "cd src-nuxt && npm install",
    "install-python": "node install/install-python.js ",
    "postinstall": "npm run install-nuxt && npm run install-python"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@tauri-apps/api": "^1.5.3",
    "execa": "^8.0.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.11",
    "cross-env": "^7.0.3",
    "shx": "^0.3.4",
    "vite-plugin-vuetify": "^2.0.3",
    "vuetify": "^3.5.16"
  }
}
